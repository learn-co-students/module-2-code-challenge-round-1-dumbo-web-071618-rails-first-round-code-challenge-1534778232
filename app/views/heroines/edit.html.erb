<h1>Edit Heroine:</h1>

<%= form_for @heroine do |f| %>
<br>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :super_name %>
<%= f.text_field :super_name %>
<br>
<%= f.label :power_id %>
<%= f.collection_select :power_id, Power.all, :id, :name %>
<br>
<%= f.submit "Edit heroine" %>
<% end %>
