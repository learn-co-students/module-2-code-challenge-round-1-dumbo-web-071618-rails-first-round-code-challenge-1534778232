<% if flash[:errors] %>
  <% flash[:errors].each do |error|  %>
<li><%= error %></li><br/>
<% end %>
<% end %>
<br/><br/>

<%= form_for(@heroine) do |f| %>
<%= f.label :name %><br/>
<%= f.text_field :name %><br/><br/>
<%= f.label :super_name %><br/>
<%= f.text_field :super_name %><br/><br/>
<%= f.collection_select :power_id, Power.all, :id, :name %><br/><br/>
<%= f.submit %>
<% end %>

<% flash.clear %>
