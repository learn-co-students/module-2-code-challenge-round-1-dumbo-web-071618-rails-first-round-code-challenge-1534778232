<h1>Create a Heroine</h1>

<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
    <li><%= error %></li>
    <br>
  <% end %>
<% end %>

<%= form_for (@heroine) do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %>
  <%= f.label :power_id %>
  <%= f.collection_select :power_id, Power.all, :id, :name %>
  <%= f.submit %>
<% end %>
