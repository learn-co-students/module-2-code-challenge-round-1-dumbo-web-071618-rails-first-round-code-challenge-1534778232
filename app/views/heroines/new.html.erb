
<%= form_for :heroine, url: "/heroines", method: "post" do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :super_name %>
    <%= f.text_field :super_name %><br>
    
    <%= f.label :power %>
    <%= f.collection_select :power_id, Power.all, :id, :name %><br>

    <%= f.submit %>
<% end %>
  